<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml">
<a:button name="btn-preview-nav">
    <a:style><![CDATA[
        .btn-preview-nav {
            height             : 23px;
            overflow           : hidden;
            cursor             : default;
            position           : relative;
            background-repeat:no-repeat;
            background-position: 0 0;
            -moz-user-select: -moz-none;
            -khtml-user-select : none;
            -webkit-user-select: none;
            -o-user-select: none;
            user-select        : none;
            
            background: url(images/c9-preview-repeat.png) repeat-x 0 100px;
            border-radius: 3px;
            
            border: 1px solid transparent;
        }

        .btn-preview-navOver {
            background-position : 0 0;
            border-color: #010910;
            box-shadow: 0 0 1px rgba(255,255,255,0.5) inset;
        }

        .btn-preview-navDown {
            /*background-position : 0 -33px;*/
            border-color: #010910;
            box-shadow: 1px 1px 1px rgba(0,0,0, 0.3) inset;
        }

        .btn-preview-nav .icon {
            width                 : 24px;
            height                : 23px;
            -webkit-border-radius : 3px;
            -moz-border-radius    : 3px;
            border-radius         : 3px;
            margin-left: 2px;
        }
        
        .btn-preview-nav.close .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -66px;
        }
        
        .btn-preview-nav.refresh .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -89px;
        }
        
        .btn-preview-nav.popout .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -114px;
        }
        
        .btn-preview-nav.goforward .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -186px;
        }
        
        .btn-preview-navDisabled.goforward .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -210px;
        }
        
        .btn-preview-nav.goback .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -138px;
        }
        
        .btn-preview-navDisabled.goback .icon {
            background: url(images/c9-preview-repeat.png) no-repeat 0 -162px;
        }
    ]]></a:style>

     <a:presentation>
        <a:main caption="div" label="div" background="." icon="div">
            <div class="btn-preview-nav">
                <div class="icon"/>
            </div>
        </a:main>
    </a:presentation>
</a:button>
<a:button name="btn-preview-choice">
    <a:style><![CDATA[
        .btn-preview-choice {
            -moz-user-select: -moz-none;
            -khtml-user-select : none;
            -webkit-user-select: none;
            -o-user-select: none;
            user-select        : none;
            cursor: default;
            line-height: 19px;
            padding-right: 17px;
            font-size: 11px;
            font-weight: bold;
            color: #333;
            font-family: Arial;
            -webkit-font-smoothing: auto;
            -moz-osx-font-smoothing: auto;
            background: rgb(190, 190, 190);
            height: 20px;
            border-radius: 0 2px 2px 0;
            box-shadow: inset 0 2px 3px 0 rgba(0,0,0,.3);
            cursor: pointer;
            border: 1px solid #000;
            border-left : 0;
            border-radius: 0 3px 3px 0;
            position : relative;
        }
        .btn-preview-choiceOver{
            background: rgb(200, 200, 200);
        }
        .btn-preview-choiceDown{
            background: rgb(210, 210, 210);
        }
        
        .btn-preview-choice span{
            display : block;
            position : absolute;
            right : 6px;
            top : 11px;
            width : 5px;
            height : 5px;
            background: url(images/arrow_down_black.png) no-repeat;
            opacity: 0.6;
        }

        .btn-preview-choiceIcon {
            background-repeat: no-repeat;
            padding-left: 25px;
            padding-top: 3px;
            background-position: 5px 4px;
        }

        .btn-preview-choice.btn-terminal {
            color: #909090;
            font-size: 11px;
            text-shadow: 0 1px 0 rgba(0,0,0,.25);
        }
        .btn-preview-choice.btn-terminal.btn-preview-choiceOver {
            color: #c5d7bf;
        }
        .btn-preview-choice.btn-terminal.btn-preview-choiceDown {
            color: #d1ebaf;
        }
    ]]></a:style>

    <a:presentation>
        <a:main
          caption    = "text()"
          background = "."
          icon       = ".">
            <div class="btn-preview-choice">
                -<span> </span>
            </div>
        </a:main>
    </a:presentation>
</a:button>
</a:skin>
